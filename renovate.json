{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":disableRateLimiting"],
  "timezone": "Asia/Tokyo",
  "automerge": true,
  "platformAutomerge": true,
  "platformCommit": true,
  "packageRules": [
    {
      "groupName": "mkdocs",
      "matchPackageNames": ["mkdocs"],
      "matchPackagePatterns": ["^mkdocs-"]
    }
  ]
}

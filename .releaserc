{
  "branches": [
    "main"
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits",
        "releaseRules": [
          {
            "type": "breaking",
            "release": "major"
          },
          {
            "type": "feat",
            "release": "minor"
          },
          {
            "type": "fix",
            "release": "patch"
          },
          {
            "type": "docs",
            "release": "patch"
          },
          {
            "type": "chore",
            "release": "patch"
          },
          {
            "type": "pref",
            "release": "patch"
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "type": "build",
            "release": "patch"
          },
          {
            "type": "ci",
            "release": "patch"
          },
          {
            "type": "revert",
            "release": "patch"
          },
          {
            "type": "style",
            "release": "patch"
          },
          {
            "type": "test",
            "release": "patch"
          }
        ],
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES"
          ]
        }
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "types": [
          {
            "type": "breaking",
            "section": "üí£ Breaking Changes"
          },
          {
            "type": "feat",
            "section": "‚ú® Features"
          },
          {
            "type": "fix",
            "section": "üõ†Ô∏è Bug Fixes"
          },
          {
            "type": "docs",
            "section": "üìÉ Docs"
          },
          {
            "type": "chore",
            "section": "üé® Chore"
          },
          {
            "type": "pref",
            "section": "üéà Performance improvements"
          },
          {
            "type": "refactor",
            "section": "üßπ Refactoring"
          },
          {
            "type": "build",
            "section": "üîç Build"
          },
          {
            "type": "ci",
            "section": "üîç CI"
          },
          {
            "type": "revert",
            "section": "‚è™ Revert"
          },
          {
            "type": "style",
            "section": "üßπ Style"
          },
          {
            "type": "test",
            "section": "üëÄ Test"
          }
        ]
      }
    ],
    "@semantic-release/github",
    "@semantic-release/git"
  ],
  "dryRun": true
}